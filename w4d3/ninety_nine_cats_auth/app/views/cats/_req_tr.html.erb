<tr>
  <% cat_count = current_user.cats.where(id: params[:id]).count %>
  <td><%= req.start_date %></td>
  <td><%= req.end_date %></td>
  <td>
    <% if req.pending? && cat_count != 0%>
        <%= button_to "Approve", approve_cat_rental_request_url(req) %>
        <%= button_to "Deny", deny_cat_rental_request_url(req) %>
    <% else %>
      <%= req.status %>
    <% end %>
  </td>
  <td><%= req.requestor.user_name %></td>
</tr>
